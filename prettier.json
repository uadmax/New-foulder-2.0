{
 "printWidth": 100,
 "tabWidth": 2,
 "useTabs": false,
 "semi": true,
 "singleQuote": true,
 "trailingComma": "all",
 "bracketSpacing": true,
 "jsxBracketSameLine": false,
 "arrowParens": "avoid",
 "proseWrap": "always",
 "vueIndentScriptAndStyle": true,
 "attr-value-doble-quotes": true,
 "tag-pair": true,
 "tagname-lowercase": true,
 "doctype-first": false,
 "id-unique": true
}